<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" immediate="true" name="com.ttn.slingexporter.service.impl.PageDataComposeServiceImpl" activate="activate">
    <property name="template.path">
        invest-india/components/page/one-column-page=col-1
        invest-india/components/page/two-column-page=col-1;col-2
        invest-india/components/page/blogpage=blogcomponent
        foundation/components/table=tableData
    </property>
    <service servicefactory="false">
        <provide interface="com.ttn.slingexporter.service.PageDataComposeService"/>
    </service>
    <reference name="pageComponentProcessorService" interface="com.ttn.slingexporter.service.PageComponentProcessorService" cardinality="1..1" policy="static" bind="bindPageComponentProcessorService" unbind="unbindPageComponentProcessorService"/>
    <reference name="componentPropertiesService" interface="com.ttn.slingexporter.service.ComponentPropertiesService" cardinality="1..1" policy="static" bind="bindComponentPropertiesService" unbind="unbindComponentPropertiesService"/>
    <implementation class="com.ttn.slingexporter.service.impl.PageDataComposeServiceImpl"/>
</scr:component>
